<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="constractor-arg" class="com.test02.Emp">
		<constructor-arg name="name" value="영태누나"></constructor-arg>
		<constructor-arg name="salary" value="100000000"></constructor-arg>
	</bean>
	
	<bean id="setter" class="com.test02.Emp">
		<property name="name" value="재필"></property>
		<property name="salary" value="100000000"></property>
	</bean>

</beans>
